{
  "kind": "collectionType",
  "collectionName": "mensajes",
  "info": {
    "name": "Mensaje"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true
    },
    "correo": {
      "type": "email",
      "required": true
    },
    "telefono": {
      "required": true,
      "type": "biginteger"
    },
    "empresa": {
      "type": "string"
    },
    "cargo": {
      "type": "string"
    },
    "descripcion": {
      "type": "text"
    }
  }
}
